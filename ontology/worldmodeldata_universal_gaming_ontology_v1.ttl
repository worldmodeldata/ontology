    skos:definition "A human individual (person) that can act in the system (e.g., a Player, Patient, Trader)." .

ug:belongsToDevice a owl:ObjectProperty ;
    rdfs:label "belongs to device"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ug:Player ;
