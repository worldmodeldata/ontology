ug:attributedTo a owl:ObjectProperty ;
    rdfs:label "attributed to"@en ;
    rdfs:domain ug:AttributedInstall ;
    rdfs:range ug:Campaign ;
    skos:definition "Relates an install to the campaign that drove it" .

ug:bidPrice a owl:DatatypeProperty ;
    rdfs:label "bid price"@en ;
    rdfs:domain ug:Bid ;
    rdfs:range xsd:decimal ;
    skos:definition "Price of the bid in the ad auction" .

ug:businessDefinition a owl:AnnotationProperty ;
    rdfs:label "business definition"@en ;
    rdfs:comment "Plain-language business definition of a concept (multi-language supported via language tags)."@en .

ug:businessImpact a owl:AnnotationProperty ;
    rdfs:label "business impact"@en ;
    rdfs:comment "Business value/impact statement for the concept (e.g., HIGH/MEDIUM/LOW with rationale)."@en .

ug:businessRationale a owl:AnnotationProperty ;
    rdfs:label "business rationale"@en ;
    rdfs:comment "Why this concept matters from a business perspective (multi-language supported via language tags)."@en .

ug:businessTerms a owl:AnnotationProperty ;
    rdfs:label "business terms"@en ;
    rdfs:comment "Business terminology/synonyms used for the concept (multi-language supported via language tags)."@en .

ug:businessThreshold a owl:AnnotationProperty ;
    rdfs:label "business threshold"@en ;
    rdfs:comment "Business thresholds/criteria used to operationalize the concept (multi-language supported via language tags)."@en .

ug:businessUseCase a owl:AnnotationProperty ;
    rdfs:label "business use case"@en ;
    rdfs:comment "Real-world business use cases for the concept (multi-language supported via language tags)."@en .

ug:campaignId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "campaign ID"@en ;
    rdfs:domain ug:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a campaign" .

ug:containsBid a owl:ObjectProperty ;
    rdfs:label "contains bid"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ug:Bid ;
    skos:definition "Relates a session to bids that occurred during it" .

ug:containsEvent a owl:ObjectProperty ;
    rdfs:label "contains event"@en ;
    rdfs:domain ug:GameSession ;
    rdfs:range ug:GameEvent ;
    owl:inverseOf ug:occursInSession ;
    skos:definition "Relates a gaming session to events it contains" .

ug:contributedBy a owl:AnnotationProperty ;
    rdfs:label "contributed by"@en ;
    rdfs:comment "Provenance: identifier of the contributor (domain expert, ontology editor, etc.)."@en .

ug:contributionDate a owl:AnnotationProperty ;
    rdfs:label "contribution date"@en ;
    rdfs:comment "Provenance: date of contribution (xsd:date)."@en .

ug:country a owl:DatatypeProperty ;
    rdfs:label "country"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:string ;
    skos:definition "Country code of the device" .

ug:currency a owl:DatatypeProperty ;
    rdfs:label "currency"@en ;
    rdfs:domain ug:MonetizationEvent,
        ug:Transaction ;
    rdfs:range xsd:string ;
    skos:definition "Currency code",
        "Currency code for the transaction" .

ug:deviceId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "device ID"@en ;
    rdfs:comment "Maps to ub:actorId in universal foundation." ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a device",
        "Unique identifier for a player/device" .

ug:deviceOS a owl:DatatypeProperty ;
    rdfs:label "device OS"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:string ;
    skos:definition "Operating system of the device" .

ug:deviceType a owl:DatatypeProperty ;
    rdfs:label "device type"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:string ;
    skos:definition "Type of device (phone, tablet, etc.)" .

ug:dolphinIapThreshold a owl:DatatypeProperty ;
