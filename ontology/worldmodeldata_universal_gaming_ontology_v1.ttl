<http://ontology.gaming.network/universal/bridge> a owl:Ontology ;
    rdfs:comment "Alignment axioms mapping core gaming ontology terms to universal foundations." .

ug:AdViewed a owl:Class ;
    rdfs:label "Ad Viewed"@en ;
    rdfs:subClassOf ug:MonetizationEvent ;
    skos:definition "Advertisement impression event" .

ug:GameEnd a owl:Class ;
    rdfs:label "Game End"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Game completion event" .

ug:GameStart a owl:Class ;
    rdfs:label "Game Start"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Game initialization event" .

ug:LevelComplete a owl:Class ;
    rdfs:label "Level Complete"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level completion event" .

ug:LevelFail a owl:Class ;
    rdfs:label "Level Fail"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level failure event" .

ug:LevelStart a owl:Class ;
    rdfs:label "Level Start"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level initialization event" .

ug:ProgressionEvent a owl:Class ;
    rdfs:label "Progression Event"@en ;
    rdfs:comment "Events related to player progression." ;
    rdfs:subClassOf ug:GameEvent ;
    skos:definition "Progression events (LevelUp, SkillUnlock, Milestone)" .

ug:Purchase a owl:Class ;
    rdfs:label "Purchase"@en ;
    rdfs:subClassOf ug:MonetizationEvent ;
    skos:definition "Purchase event (in-app purchase, subscription)" .

ug:SocialEvent a owl:Class ;
    rdfs:label "Social Event"@en ;
    rdfs:comment "Events related to social interactions in games." ;
    rdfs:subClassOf ug:GameEvent ;
    skos:definition "Social events (MultiplayerJoin, FriendInvite, ChatMessage)" .

ug:difficulty a owl:DatatypeProperty ;
    rdfs:label "difficulty"@en ;
    rdfs:domain ug:EngagementEvent ;
    rdfs:range xsd:string ;
    skos:definition "Difficulty level (easy, medium, hard)" .

ug:engagementScore a owl:DatatypeProperty ;
    rdfs:label "engagement score"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:decimal ;
    skos:definition "Calculated engagement metric" .

ug:gameGenre a owl:DatatypeProperty ;
    rdfs:label "game genre"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Genre of the game (Action, RPG, Puzzle, etc.)" .

ug:gameId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "game ID"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a game" .

ug:gameName a owl:DatatypeProperty ;
    rdfs:label "game name"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Name of the game" .

ug:hasPlayerState a owl:ObjectProperty ;
    rdfs:label "has player state"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:PlayerState ;
    skos:definition "Relates a player to their current state" .

ug:itemId a owl:DatatypeProperty ;
    rdfs:label "item ID"@en ;
    rdfs:domain ug:MonetizationEvent ;
    rdfs:range xsd:string ;
    skos:definition "Item/product identifier" .

ug:levelNumber a owl:DatatypeProperty ;
    rdfs:label "level number"@en ;
    rdfs:domain ug:EngagementEvent ;
    rdfs:range xsd:integer ;
    skos:definition "Level identifier/number" .

ug:retentionDay a owl:DatatypeProperty ;
    rdfs:label "retention day"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Day X retention status (1, 7, 30)" .

ug:similarBehaviorTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar behavior to"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:Player ;
    skos:definition "Relates players with similar behavioral patterns (across games)" .

ug:similarEventTypeTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar event type to"@en ;
    rdfs:domain ug:GameEvent ;
    rdfs:range ug:GameEvent ;
    skos:definition "Relates similar event types across games" .

ug:similarGenreTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar genre to"@en ;
    rdfs:domain ug:Game ;
    rdfs:range ug:Game ;
    skos:definition "Relates games with similar genres" .

ug:totalIaps a owl:DatatypeProperty ;
    rdfs:label "total IAPs"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Total in-app purchases for this player" .

<http://ontology.gaming.network/universal/gaming_ontology> a owl:Ontology ;
    dc:creator "WorldModelData" ;
    dc:title "WorldModelData Universal Gaming Ontology v1"@en ;
    rdfs:comment "Flagship universal gaming ontology release. Flattened distribution containing the universal foundations plus absorbed legacy core semantics."@en ;
    owl:imports <http://ontology.gaming.network/universal/gaming>,
        <http://ontology.gaming.network/universal/human_behavior> ;
    owl:versionInfo "1.0.0" .

ub:activityDuration a owl:DatatypeProperty ;
    rdfs:label "activity duration"@en ;
    rdfs:comment "Universal temporal property for activity duration." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:integer ;
    skos:definition "Duration of the activity in seconds" .

ub:activityEndTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity end timestamp"@en ;
    rdfs:comment "Universal temporal property for activity end time." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity ended" .

ub:activityId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "activity ID"@en ;
    rdfs:comment "Universal identifier property for any activity type." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for an activity" .

ub:activityStartTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity start timestamp"@en ;
    rdfs:comment "Universal temporal property for activity start time." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity started" .

ub:activityTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:comment "Universal temporal property for any behavioral event type." ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the behavioral event occurred" .

ub:actorId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "actor ID"@en ;
    rdfs:comment "Universal identifier property for any human actor type." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a human actor" .

ub:eventId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "event ID"@en ;
    rdfs:comment "Universal identifier property for any behavioral event type." ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a behavioral event" .

ub:generatesEvent a owl:ObjectProperty ;
    rdfs:label "generates event"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ub:BehavioralEvent ;
    owl:inverseOf ub:generatedByActivity ;
    skos:definition "Relates an activity to behavioral events it generates" .

ub:hasEngagementLevel a owl:ObjectProperty ;
    rdfs:label "has engagement level"@en ;
    rdfs:comment "Universal property for engagement classification across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to gaming (player engagement), treatment (patient engagement), task performance (user engagement)" .

ub:hasRetentionStatus a owl:ObjectProperty ;
    rdfs:label "has retention status"@en ;
    rdfs:comment "Universal property for retention classification across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to player retention (gaming), treatment adherence (healthcare), customer retention (finance)" .

ub:hasSocialContext a owl:ObjectProperty ;
    rdfs:label "has social context"@en ;
    rdfs:comment "Universal property for social context across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to multiplayer games (gaming), social support (healthcare), network effects (finance)" .

ub:hasState a owl:ObjectProperty ;
    rdfs:label "has state"@en ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    owl:inverseOf ub:stateOfActor ;
    skos:definition "Relates a human actor to their behavioral state" .

ub:hasTemporalPattern a owl:ObjectProperty ;
    rdfs:label "has temporal pattern"@en ;
    rdfs:comment "Universal property for temporal behavior patterns across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to session frequency (gaming), treatment frequency (healthcare), activity frequency (finance)" .

ub:hasValueSegment a owl:ObjectProperty ;
    rdfs:label "has value segment"@en ;
    rdfs:comment "Universal property for value segmentation across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to monetization (gaming), treatment compliance (healthcare), customer value (finance)" .

ub:influencesState a owl:ObjectProperty ;
    rdfs:label "influences state"@en ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range ub:BehavioralState ;
    owl:inverseOf ub:influencedByEvent ;
    skos:definition "Relates a behavioral event to behavioral states it influences" .

ub:performsActivity a owl:ObjectProperty ;
    rdfs:label "performs activity"@en ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:Activity ;
    owl:inverseOf ub:performedByActor ;
    skos:definition "Relates a human actor to activities they perform" .

ub:similarTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar to"@en ;
    rdfs:comment "Used for behavioral similarity analysis and pattern recognition." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:HumanActor ;
    skos:definition "Relates human actors with similar behavioral patterns" .

<http://ontology.gaming.network/universal/gaming> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-12-27" ;
    dc:title "Universal Gaming Foundation Ontology v1.0" ;
    rdfs:comment "Comprehensive gaming ontology extending universal human behavior foundation" ;
    owl:imports <http://ontology.gaming.network/universal/human_behavior> ;
    owl:versionInfo "1.0.0" .

ug:Human a owl:Class ;
    rdfs:label "Human"@en ;
    rdfs:subClassOf ug:Person ;
    skos:definition "A Human being" .

ug:Resource a owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:subClassOf ug:Entity ;
    skos:definition "A resource used or consumed in the gaming ecosystem" .

ug:activityTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the event occurred" .

ug:advertiserDomain a owl:DatatypeProperty ;
    rdfs:label "advertiser domain"@en ;
    rdfs:domain ug:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Domain of the advertiser" .

ug:amount a owl:DatatypeProperty ;
    rdfs:label "amount"@en ;
    rdfs:domain ug:MonetizationEvent,
        ug:Transaction ;
    rdfs:range xsd:decimal ;
    skos:definition "Transaction amount" .

ug:appliesToTenant a owl:DatatypeProperty ;
    rdfs:label "applies to tenant"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:string ;
