ug:dolphinMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "dolphin monetary threshold"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Dolphin (future use)"@en .

ug:eventType a owl:DatatypeProperty ;
    rdfs:label "event type"@en ;
    rdfs:domain ug:GameEvent,
        ub:BehavioralEvent ;
    rdfs:range xsd:string ;
    skos:definition "Type of event",
        "Type of gaming event" .

ug:hasAdInteraction a owl:ObjectProperty ;
    rdfs:label "has ad interaction"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ug:AdInteraction ;
    skos:definition "Relates a session to ad interactions during it" .

ug:hasAttributedInstall a owl:ObjectProperty ;
    rdfs:label "has attributed install"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:AttributedInstall ;
    skos:definition "Relates a device to its attributed install event" .

ug:hasEngagementLevel a owl:ObjectProperty ;
    rdfs:label "has engagement level"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:EngagementLevel ;
    skos:definition "Relates a device to its inferred engagement classification",
        "Relates a player to their engagement level classification" .

ug:hasImpression a owl:ObjectProperty ;
    rdfs:label "has impression"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ug:Impression ;
    skos:definition "Relates a session to ad impressions shown during it" .

ug:hasInAppPurchase a owl:ObjectProperty ;
    rdfs:label "has in-app purchase"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ug:InAppPurchase ;
    skos:definition "Relates a session to purchases made during it" .

ug:hasMonetizationSegment a owl:ObjectProperty ;
    rdfs:label "has monetization segment"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:MonetizationSegment ;
    skos:definition "Relates a device to its inferred monetization classification",
        "Relates a player to their monetization segment classification" .

ug:hasRetentionStatus a owl:ObjectProperty ;
    rdfs:label "has retention status"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:RetentionStatus ;
    skos:definition "Relates a device to its inferred retention status",
        "Relates a player to their retention status classification" .

ug:hasSession a owl:ObjectProperty ;
    rdfs:label "has session"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:GameSession,
        ub:Activity ;
    owl:inverseOf ug:belongsToDevice,
        ug:belongsToPlayer ;
    skos:definition "Relates a device to its sessions",
        "Relates a player to their gaming sessions" .

ug:lastReviewed a owl:AnnotationProperty ;
    rdfs:label "last reviewed"@en ;
    rdfs:comment "Provenance: date this business semantics annotation was last reviewed (xsd:date)."@en .

ug:minnowIapThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow IAP threshold"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Minnow"@en .

ug:minnowMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow monetary threshold"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Minnow (future use)"@en .

ug:occursInGame a owl:ObjectProperty ;
    rdfs:label "occurs in game"@en ;
    rdfs:domain ug:GameSession,
        ub:Activity ;
    rdfs:range ug:Game ;
