ug:belongsToPlayer a owl:ObjectProperty ;
    rdfs:label "belongs to player"@en ;
    rdfs:domain ug:GameSession ;
    rdfs:range ug:Player ;
    skos:definition "Relates a gaming session to the player" .

ug:occursInSession a owl:ObjectProperty ;
    rdfs:label "occurs in session"@en ;
    rdfs:domain ug:GameEvent ;
    rdfs:range ug:GameSession ;
    skos:definition "Relates a gaming event to the session it occurs in" .

<http://ontology.gaming.network/universal/human_behavior> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-12-27" ;
    dc:title "Universal Human Behavior Foundation" ;
    rdfs:comment "Universal extensible foundation for human behavior data across domains" ;
    owl:versionInfo "1.0.0" .

ug:ActivePlayer a owl:Class ;
    rdfs:label "Active Player"@en ;
    rdfs:subClassOf ug:PlayerState ;
    skos:definition "Currently active player" .

ug:Actor a owl:Class ;
    rdfs:label "Actor"@en ;
    rdfs:subClassOf ug:Entity ;
    skos:definition "An entity that can perform actions in the gaming ecosystem" .

ug:AttributedInstall a owl:Class ;
    rdfs:label "Attributed Install"@en ;
    rdfs:subClassOf ub:BehavioralEvent ;
    skos:definition "A game installation attributed to a campaign" .

ug:Bid a owl:Class ;
