    rdfs:range ug:Game ;
    skos:definition "Relates a device to games it plays",
        "Relates a player to games they play" .

ug:sessionCount a owl:DatatypeProperty ;
    rdfs:label "session count"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Number of gaming sessions for this player",
        "Total number of sessions for this device" .

ug:sessionDuration a owl:DatatypeProperty ;
    rdfs:label "session duration"@en ;
    rdfs:comment "Maps to ub:activityDuration in universal foundation." ;
    rdfs:domain ug:GameSession ;
    rdfs:range xsd:integer ;
    skos:definition "Duration of gaming session in seconds" .

ug:sessionId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "session ID"@en ;
    rdfs:comment "Maps to ub:activityId in universal foundation." ;
    rdfs:domain ug:GameSession,
        ub:Activity ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a gaming session",
        "Unique identifier for a session" .

ug:totalSpent a owl:DatatypeProperty ;
    rdfs:label "total spent"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:decimal ;
    skos:definition "Total amount spent by this device" .

ug:whaleIapThreshold a owl:DatatypeProperty ;
    rdfs:label "whale IAP threshold"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Whale"@en .

ug:whaleMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "whale monetary threshold"@en ;
    rdfs:domain ug:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Whale (future use)"@en .

ub:generatedByActivity a owl:ObjectProperty ;
    rdfs:label "generated by activity"@en ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range ub:Activity ;
    skos:definition "Relates a behavioral event to the activity that generated it" .

ub:influencedByEvent a owl:ObjectProperty ;
    rdfs:label "influenced by event"@en ;
    rdfs:domain ub:BehavioralState ;
    rdfs:range ub:BehavioralEvent ;
    skos:definition "Relates a behavioral state to events that influence it" .

ub:performedByActor a owl:ObjectProperty ;
    rdfs:label "performed by actor"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ub:HumanActor ;
    skos:definition "Relates an activity to the human actor performing it" .

ub:stateOfActor a owl:ObjectProperty ;
    rdfs:label "state of actor"@en ;
    rdfs:domain ub:BehavioralState ;
    rdfs:range ub:HumanActor ;
    skos:definition "Relates a behavioral state to the human actor it describes" .

ug:AdInteraction a owl:Class ;
    rdfs:label "Ad Interaction"@en ;
    rdfs:subClassOf ug:AdEvent ;
    skos:definition "User interaction with an advertisement" .

ug:Impression a owl:Class ;
    rdfs:label "Impression"@en ;
    rdfs:subClassOf ug:AdEvent ;
    skos:definition "An ad impression shown to a user" .

ug:InAppPurchase a owl:Class ;
    rdfs:label "In-App Purchase"@en ;
    rdfs:subClassOf ug:Transaction ;
    skos:altLabel "IAP"@en ;
