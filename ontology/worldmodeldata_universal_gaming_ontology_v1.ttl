ug:LowEngagement a owl:Class ;
    rdfs:label "Low Engagement"@en ;
    rdfs:subClassOf ug:EngagementLevel ;
    skos:definition "User with low engagement (1-2 sessions)" .

ug:MediumEngagement a owl:Class ;
    rdfs:label "Medium Engagement"@en ;
    rdfs:subClassOf ug:EngagementLevel ;
    skos:definition "User with medium engagement (3-9 sessions)" .

ug:Minnow a owl:Class ;
    rdfs:label "Minnow"@en ;
    rdfs:subClassOf ug:MonetizationSegment ;
    skos:definition "User with low spending",
        "User with low spending (£0-£10)" .

ug:NonPayer a owl:Class ;
    rdfs:label "Non-Payer"@en ;
    rdfs:subClassOf ug:MonetizationSegment ;
    skos:definition "User who has never made a purchase" .

ug:Retained a owl:Class ;
    rdfs:label "Retained"@en ;
    rdfs:subClassOf ug:RetentionStatus ;
    skos:definition "User who continues to engage regularly" .

ug:Transaction a owl:Class ;
    rdfs:label "Transaction"@en ;
    rdfs:subClassOf ub:BehavioralEvent ;
    skos:definition "A financial transaction" .

ug:Whale a owl:Class ;
    rdfs:label "Whale"@en ;
    rdfs:subClassOf ug:MonetizationSegment ;
    skos:definition "User with high spending",
        "User with high spending (£50+)" .

ug:AdEvent a owl:Class ;
    rdfs:label "Ad Event"@en ;
    rdfs:subClassOf ub:BehavioralEvent ;
