@prefix core: <http://ontology.gaming.network/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ub: <http://ontology.gaming.network/universal/human_behavior#> .
@prefix ug: <http://ontology.gaming.network/universal/gaming#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://ontology.gaming.network/core> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-11-24" ;
    dc:title "Gaming Network Core Ontology (DEPRECATED)" ;
    rdfs:comment "DEPRECATED: legacy core ontology. Use /ontology/worldmodeldata_universal_gaming_ontology_v1.ttl (WorldModelData Universal Gaming Ontology v1) as the flagship universal ontology distribution." ;
    owl:deprecated true ;
    owl:versionInfo "1.0.0" .

core:GameEvent owl:equivalentClass ug:GameEvent .

core:GameSession a owl:Class ;
    rdfs:label "Game Session"@en ;
    rdfs:subClassOf core:Session ;
    owl:equivalentClass ug:GameSession ;
    skos:definition "A session within a specific game" .

core:Human a owl:Class ;
    rdfs:label "New Class"@en ;
    rdfs:subClassOf core:Person ;
    skos:definition "A Human being" .

core:Player owl:equivalentClass ug:Player .

core:Resource a owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:subClassOf core:Entity ;
    skos:definition "A resource used or consumed in the gaming ecosystem" .

core:TEST a owl:Class ;
    rdfs:label "Test Class"@en ;
    rdfs:subClassOf core:Event ;
    skos:definition "A test class" .

core:activityTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:domain core:Event ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the event occurred" .

core:advertiserDomain a owl:DatatypeProperty ;
    rdfs:label "advertiser domain"@en ;
    rdfs:domain core:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Domain of the advertiser" .

core:amount a owl:DatatypeProperty ;
    rdfs:label "amount"@en ;
    rdfs:domain core:Transaction ;
    rdfs:range xsd:decimal ;
    skos:definition "Transaction amount" .

core:appliesToTenant a owl:DatatypeProperty ;
    rdfs:label "applies to tenant"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:string ;
    skos:definition "Tenant identifier this threshold configuration applies to"@en .

core:attributedTo a owl:ObjectProperty ;
    rdfs:label "attributed to"@en ;
    rdfs:domain core:AttributedInstall ;
    rdfs:range core:Campaign ;
    skos:definition "Relates an install to the campaign that drove it" .

core:belongsToPlayer owl:equivalentProperty ug:belongsToPlayer .

core:bidPrice a owl:DatatypeProperty ;
    rdfs:label "bid price"@en ;
    rdfs:domain core:Bid ;
    rdfs:range xsd:decimal ;
    skos:definition "Price of the bid in the ad auction" .

core:businessDefinition a owl:AnnotationProperty ;
    rdfs:label "business definition"@en ;
    rdfs:comment "Plain-language business definition of a concept (multi-language supported via language tags)."@en .

core:businessImpact a owl:AnnotationProperty ;
    rdfs:label "business impact"@en ;
    rdfs:comment "Business value/impact statement for the concept (e.g., HIGH/MEDIUM/LOW with rationale)."@en .

core:businessRationale a owl:AnnotationProperty ;
    rdfs:label "business rationale"@en ;
    rdfs:comment "Why this concept matters from a business perspective (multi-language supported via language tags)."@en .

core:businessTerms a owl:AnnotationProperty ;
    rdfs:label "business terms"@en ;
    rdfs:comment "Business terminology/synonyms used for the concept (multi-language supported via language tags)."@en .

core:businessThreshold a owl:AnnotationProperty ;
    rdfs:label "business threshold"@en ;
    rdfs:comment "Business thresholds/criteria used to operationalize the concept (multi-language supported via language tags)."@en .

core:businessUseCase a owl:AnnotationProperty ;
    rdfs:label "business use case"@en ;
    rdfs:comment "Real-world business use cases for the concept (multi-language supported via language tags)."@en .

core:campaignId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "campaign ID"@en ;
    rdfs:domain core:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a campaign" .

core:containsBid a owl:ObjectProperty ;
    rdfs:label "contains bid"@en ;
    rdfs:domain core:Session ;
    rdfs:range core:Bid ;
    skos:definition "Relates a session to bids that occurred during it" .

core:containsEvent owl:equivalentProperty ug:containsEvent .

core:contributedBy a owl:AnnotationProperty ;
    rdfs:label "contributed by"@en ;
    rdfs:comment "Provenance: identifier of the contributor (domain expert, ontology editor, etc.)."@en .

core:contributionDate a owl:AnnotationProperty ;
    rdfs:label "contribution date"@en ;
    rdfs:comment "Provenance: date of contribution (xsd:date)."@en .

core:country a owl:DatatypeProperty ;
    rdfs:label "country"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:string ;
    skos:definition "Country code of the device" .

core:currency a owl:DatatypeProperty ;
    rdfs:label "currency"@en ;
    rdfs:domain core:Transaction ;
    rdfs:range xsd:string ;
    skos:definition "Currency code for the transaction" .

core:deviceId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "device ID"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:string ;
    owl:equivalentProperty ug:deviceId ;
    skos:definition "Unique identifier for a device" .

core:deviceOS a owl:DatatypeProperty ;
    rdfs:label "device OS"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:string ;
    skos:definition "Operating system of the device" .

core:deviceType a owl:DatatypeProperty ;
    rdfs:label "device type"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:string ;
    skos:definition "Type of device (phone, tablet, etc.)" .

core:dolphinIapThreshold a owl:DatatypeProperty ;
    rdfs:label "dolphin IAP threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Dolphin"@en .

core:dolphinMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "dolphin monetary threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Dolphin (future use)"@en .

core:eventType a owl:DatatypeProperty ;
    rdfs:label "event type"@en ;
    rdfs:domain core:Event ;
    rdfs:range xsd:string ;
    skos:definition "Type of event" .

core:hasAdInteraction a owl:ObjectProperty ;
    rdfs:label "has ad interaction"@en ;
    rdfs:domain core:Session ;
    rdfs:range core:AdInteraction ;
    skos:definition "Relates a session to ad interactions during it" .

core:hasAttributedInstall a owl:ObjectProperty ;
    rdfs:label "has attributed install"@en ;
    rdfs:domain core:Device ;
    rdfs:range core:AttributedInstall ;
    skos:definition "Relates a device to its attributed install event" .

core:hasEngagementLevel a owl:ObjectProperty ;
    rdfs:label "has engagement level"@en ;
    rdfs:domain core:Device ;
    rdfs:range core:EngagementLevel ;
    owl:equivalentProperty ug:hasEngagementLevel ;
    skos:definition "Relates a device to its inferred engagement classification" .

core:hasImpression a owl:ObjectProperty ;
    rdfs:label "has impression"@en ;
    rdfs:domain core:Session ;
    rdfs:range core:Impression ;
    skos:definition "Relates a session to ad impressions shown during it" .

core:hasInAppPurchase a owl:ObjectProperty ;
    rdfs:label "has in-app purchase"@en ;
    rdfs:domain core:Session ;
    rdfs:range core:InAppPurchase ;
    skos:definition "Relates a session to purchases made during it" .

core:hasMonetizationSegment a owl:ObjectProperty ;
    rdfs:label "has monetization segment"@en ;
    rdfs:domain core:Device ;
    rdfs:range core:MonetizationSegment ;
    skos:definition "Relates a device to its inferred monetization classification" .

core:hasRetentionStatus a owl:ObjectProperty ;
    rdfs:label "has retention status"@en ;
    rdfs:domain core:Device ;
    rdfs:range core:RetentionStatus ;
    owl:equivalentProperty ug:hasRetentionStatus ;
    skos:definition "Relates a device to its inferred retention status" .

core:hasSession a owl:ObjectProperty ;
    rdfs:label "has session"@en ;
    rdfs:domain core:Device ;
    rdfs:range core:Session ;
    owl:equivalentProperty ug:hasSession ;
    owl:inverseOf core:belongsToDevice ;
    skos:definition "Relates a device to its sessions" .

core:lastReviewed a owl:AnnotationProperty ;
    rdfs:label "last reviewed"@en ;
    rdfs:comment "Provenance: date this business semantics annotation was last reviewed (xsd:date)."@en .

core:minnowIapThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow IAP threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Minnow"@en .

core:minnowMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow monetary threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Minnow (future use)"@en .

core:occursInGame a owl:ObjectProperty ;
    rdfs:label "occurs in game"@en ;
    rdfs:domain core:Session ;
    rdfs:range core:Game ;
    skos:definition "Relates a session to the game it occurs in" .

core:occursInSession owl:equivalentProperty ug:occursInSession .

core:partOfCampaign a owl:ObjectProperty ;
    rdfs:label "part of campaign"@en ;
    rdfs:domain core:AdEvent ;
    rdfs:range core:Campaign ;
    skos:definition "Relates an ad event to its campaign" .

core:playsGame a owl:ObjectProperty ;
    rdfs:label "plays game"@en ;
    rdfs:comment "Inferred from hasSession and occursInGame" ;
    rdfs:domain core:Device ;
    rdfs:range core:Game ;
    skos:definition "Relates a device to games it plays" .

core:sessionCount a owl:DatatypeProperty ;
    rdfs:label "session count"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:integer ;
    skos:definition "Total number of sessions for this device" .

core:sessionDuration owl:equivalentProperty ug:sessionDuration .

core:sessionId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "session ID"@en ;
    rdfs:domain core:Session ;
    rdfs:range xsd:string ;
    owl:equivalentProperty ug:sessionId ;
    skos:definition "Unique identifier for a session" .

core:testProperty a owl:DatatypeProperty ;
    rdfs:label "Test Property"@en ;
    rdfs:domain core:TestClass ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#xsd:string> ;
    skos:definition "Test property for E2E testing" .

core:totalSpent a owl:DatatypeProperty ;
    rdfs:label "total spent"@en ;
    rdfs:domain core:Device ;
    rdfs:range xsd:decimal ;
    skos:definition "Total amount spent by this device" .

core:whaleIapThreshold a owl:DatatypeProperty ;
    rdfs:label "whale IAP threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Whale"@en .

core:whaleMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "whale monetary threshold"@en ;
    rdfs:domain core:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Whale (future use)"@en .

<http://ontology.gaming.network/universal/bridge> a owl:Ontology ;
    rdfs:comment "Alignment axioms mapping core gaming ontology terms to universal foundations." .

<http://ontology.gaming.network/universal/gaming> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-12-27" ;
    dc:title "Universal Gaming Foundation Ontology v1.0" ;
    rdfs:comment "Comprehensive gaming ontology extending universal human behavior foundation" ;
    owl:imports <http://ontology.gaming.network/universal/human_behavior> ;
    owl:versionInfo "1.0.0" .

ug:AdViewed a owl:Class ;
    rdfs:label "Ad Viewed"@en ;
    rdfs:subClassOf ug:MonetizationEvent ;
    skos:definition "Advertisement impression event" .

ug:GameEnd a owl:Class ;
    rdfs:label "Game End"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Game completion event" .

ug:GameStart a owl:Class ;
    rdfs:label "Game Start"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Game initialization event" .

ug:LevelComplete a owl:Class ;
    rdfs:label "Level Complete"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level completion event" .

ug:LevelFail a owl:Class ;
    rdfs:label "Level Fail"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level failure event" .

ug:LevelStart a owl:Class ;
    rdfs:label "Level Start"@en ;
    rdfs:subClassOf ug:EngagementEvent ;
    skos:definition "Level initialization event" .

ug:ProgressionEvent a owl:Class ;
    rdfs:label "Progression Event"@en ;
    rdfs:comment "Events related to player progression." ;
    rdfs:subClassOf ug:GameEvent ;
    skos:definition "Progression events (LevelUp, SkillUnlock, Milestone)" .

ug:Purchase a owl:Class ;
    rdfs:label "Purchase"@en ;
    rdfs:subClassOf ug:MonetizationEvent ;
    skos:definition "Purchase event (in-app purchase, subscription)" .

ug:SocialEvent a owl:Class ;
    rdfs:label "Social Event"@en ;
    rdfs:comment "Events related to social interactions in games." ;
    rdfs:subClassOf ug:GameEvent ;
    skos:definition "Social events (MultiplayerJoin, FriendInvite, ChatMessage)" .

ug:amount a owl:DatatypeProperty ;
    rdfs:label "amount"@en ;
    rdfs:domain ug:MonetizationEvent ;
    rdfs:range xsd:decimal ;
    skos:definition "Transaction amount" .

ug:currency a owl:DatatypeProperty ;
    rdfs:label "currency"@en ;
    rdfs:domain ug:MonetizationEvent ;
    rdfs:range xsd:string ;
    skos:definition "Currency code" .

ug:difficulty a owl:DatatypeProperty ;
    rdfs:label "difficulty"@en ;
    rdfs:domain ug:EngagementEvent ;
    rdfs:range xsd:string ;
    skos:definition "Difficulty level (easy, medium, hard)" .

ug:engagementScore a owl:DatatypeProperty ;
    rdfs:label "engagement score"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:decimal ;
    skos:definition "Calculated engagement metric" .

ug:eventType a owl:DatatypeProperty ;
    rdfs:label "event type"@en ;
    rdfs:domain ug:GameEvent ;
    rdfs:range xsd:string ;
    skos:definition "Type of gaming event" .

ug:gameGenre a owl:DatatypeProperty ;
    rdfs:label "game genre"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Genre of the game (Action, RPG, Puzzle, etc.)" .

ug:gameId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "game ID"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a game" .

ug:gameName a owl:DatatypeProperty ;
    rdfs:label "game name"@en ;
    rdfs:domain ug:Game ;
    rdfs:range xsd:string ;
    skos:definition "Name of the game" .

ug:hasMonetizationSegment a owl:ObjectProperty ;
    rdfs:label "has monetization segment"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:MonetizationSegment ;
    skos:definition "Relates a player to their monetization segment classification" .

ug:hasPlayerState a owl:ObjectProperty ;
    rdfs:label "has player state"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:PlayerState ;
    skos:definition "Relates a player to their current state" .

ug:itemId a owl:DatatypeProperty ;
    rdfs:label "item ID"@en ;
    rdfs:domain ug:MonetizationEvent ;
    rdfs:range xsd:string ;
    skos:definition "Item/product identifier" .

ug:levelNumber a owl:DatatypeProperty ;
    rdfs:label "level number"@en ;
    rdfs:domain ug:EngagementEvent ;
    rdfs:range xsd:integer ;
    skos:definition "Level identifier/number" .

ug:occursInGame a owl:ObjectProperty ;
    rdfs:label "occurs in game"@en ;
    rdfs:domain ug:GameSession ;
    rdfs:range ug:Game ;
    skos:definition "Relates a gaming session to the game it occurs in" .

ug:playsGame a owl:ObjectProperty ;
    rdfs:label "plays game"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:Game ;
    skos:definition "Relates a player to games they play" .

ug:retentionDay a owl:DatatypeProperty ;
    rdfs:label "retention day"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Day X retention status (1, 7, 30)" .

ug:sessionCount a owl:DatatypeProperty ;
    rdfs:label "session count"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Number of gaming sessions for this player" .

ug:similarBehaviorTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar behavior to"@en ;
    rdfs:domain ug:Player ;
    rdfs:range ug:Player ;
    skos:definition "Relates players with similar behavioral patterns (across games)" .

ug:similarEventTypeTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar event type to"@en ;
    rdfs:domain ug:GameEvent ;
    rdfs:range ug:GameEvent ;
    skos:definition "Relates similar event types across games" .

ug:similarGenreTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar genre to"@en ;
    rdfs:domain ug:Game ;
    rdfs:range ug:Game ;
    skos:definition "Relates games with similar genres" .

ug:totalIaps a owl:DatatypeProperty ;
    rdfs:label "total IAPs"@en ;
    rdfs:domain ug:Player ;
    rdfs:range xsd:integer ;
    skos:definition "Total in-app purchases for this player" .

ub:activityDuration a owl:DatatypeProperty ;
    rdfs:label "activity duration"@en ;
    rdfs:comment "Universal temporal property for activity duration." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:integer ;
    skos:definition "Duration of the activity in seconds" .

ub:activityEndTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity end timestamp"@en ;
    rdfs:comment "Universal temporal property for activity end time." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity ended" .

ub:activityId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "activity ID"@en ;
    rdfs:comment "Universal identifier property for any activity type." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for an activity" .

ub:activityStartTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity start timestamp"@en ;
    rdfs:comment "Universal temporal property for activity start time." ;
    rdfs:domain ub:Activity ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity started" .

ub:activityTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:comment "Universal temporal property for any behavioral event type." ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the behavioral event occurred" .

ub:actorId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "actor ID"@en ;
    rdfs:comment "Universal identifier property for any human actor type." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a human actor" .

ub:eventId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "event ID"@en ;
    rdfs:comment "Universal identifier property for any behavioral event type." ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a behavioral event" .

ub:generatesEvent a owl:ObjectProperty ;
    rdfs:label "generates event"@en ;
    rdfs:domain ub:Activity ;
    rdfs:range ub:BehavioralEvent ;
    owl:inverseOf ub:generatedByActivity ;
    skos:definition "Relates an activity to behavioral events it generates" .

ub:hasEngagementLevel a owl:ObjectProperty ;
    rdfs:label "has engagement level"@en ;
    rdfs:comment "Universal property for engagement classification across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to gaming (player engagement), treatment (patient engagement), task performance (user engagement)" .

ub:hasRetentionStatus a owl:ObjectProperty ;
    rdfs:label "has retention status"@en ;
    rdfs:comment "Universal property for retention classification across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to player retention (gaming), treatment adherence (healthcare), customer retention (finance)" .

ub:hasSocialContext a owl:ObjectProperty ;
    rdfs:label "has social context"@en ;
    rdfs:comment "Universal property for social context across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to multiplayer games (gaming), social support (healthcare), network effects (finance)" .

ub:hasState a owl:ObjectProperty ;
    rdfs:label "has state"@en ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    owl:inverseOf ub:stateOfActor ;
    skos:definition "Relates a human actor to their behavioral state" .

ub:hasTemporalPattern a owl:ObjectProperty ;
    rdfs:label "has temporal pattern"@en ;
    rdfs:comment "Universal property for temporal behavior patterns across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to session frequency (gaming), treatment frequency (healthcare), activity frequency (finance)" .

ub:hasValueSegment a owl:ObjectProperty ;
    rdfs:label "has value segment"@en ;
    rdfs:comment "Universal property for value segmentation across domains." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:BehavioralState ;
    skos:definition "Applies to monetization (gaming), treatment compliance (healthcare), customer value (finance)" .

ub:influencesState a owl:ObjectProperty ;
    rdfs:label "influences state"@en ;
    rdfs:domain ub:BehavioralEvent ;
    rdfs:range ub:BehavioralState ;
    owl:inverseOf ub:influencedByEvent ;
    skos:definition "Relates a behavioral event to behavioral states it influences" .

ub:performsActivity a owl:ObjectProperty ;
    rdfs:label "performs activity"@en ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:Activity ;
    owl:inverseOf ub:performedByActor ;
    skos:definition "Relates a human actor to activities they perform" .

ub:similarTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar to"@en ;
    rdfs:comment "Used for behavioral similarity analysis and pattern recognition." ;
    rdfs:domain ub:HumanActor ;
    rdfs:range ub:HumanActor ;
    skos:definition "Relates human actors with similar behavioral patterns" .

core:AdInteraction a owl:Class ;
    rdfs:label "Ad Interaction"@en ;
    rdfs:subClassOf core:AdEvent ;
    skos:definition "User interaction with an advertisement" .

core:AtRisk a owl:Class ;
    rdfs:label "At Risk"@en ;
    rdfs:subClassOf core:RetentionStatus ;
    skos:definition "User who may churn soon" .

core:Churned a owl:Class ;
    rdfs:label "Churned"@en ;
    rdfs:subClassOf core:RetentionStatus ;
    skos:definition "User who has not returned after first session" .

core:Dolphin a owl:Class ;
    rdfs:label "Dolphin"@en ;
    rdfs:subClassOf core:MonetizationSegment ;
    skos:definition "User with medium spending (£10-£50)" .

core:HighEngagement a owl:Class ;
    rdfs:label "High Engagement"@en ;
    rdfs:subClassOf core:EngagementLevel ;
    skos:definition "User with high engagement (10+ sessions)" .

core:Impression a owl:Class ;
    rdfs:label "Impression"@en ;
    rdfs:subClassOf core:AdEvent ;
    skos:definition "An ad impression shown to a user" .

core:InAppPurchase a owl:Class ;
    rdfs:label "In-App Purchase"@en ;
    rdfs:subClassOf core:Transaction ;
    skos:altLabel "IAP"@en ;
    skos:definition "A purchase made within a game" .

core:LowEngagement a owl:Class ;
    rdfs:label "Low Engagement"@en ;
    rdfs:subClassOf core:EngagementLevel ;
    skos:definition "User with low engagement (1-2 sessions)" .

core:MediumEngagement a owl:Class ;
    rdfs:label "Medium Engagement"@en ;
    rdfs:subClassOf core:EngagementLevel ;
