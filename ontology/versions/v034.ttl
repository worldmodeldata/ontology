@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns1: <http://ontology.gaming.network/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://ontology.gaming.network/core> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-11-24" ;
    dc:title "Gaming Network Core Ontology" ;
    rdfs:comment "Formal ontology for gaming data across multiple games and ad networks" ;
    owl:versionInfo "1.0.0" .

ns1:GameEvent owl:equivalentClass <http://ontology.gaming.network/universal/gaming#GameEvent> .

ns1:GameSession a owl:Class ;
    rdfs:label "Game Session"@en ;
    rdfs:subClassOf ns1:Session ;
    owl:equivalentClass <http://ontology.gaming.network/universal/gaming#GameSession> ;
    skos:definition "A session within a specific game" .

ns1:Human a owl:Class ;
    rdfs:label "New Class"@en ;
    rdfs:subClassOf ns1:Person ;
    skos:definition "A Human being" .

ns1:Player owl:equivalentClass <http://ontology.gaming.network/universal/gaming#Player> .

ns1:Resource a owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:subClassOf ns1:Entity ;
    skos:definition "A resource used or consumed in the gaming ecosystem" .

ns1:TEST a owl:Class ;
    rdfs:label "Test Class"@en ;
    rdfs:subClassOf ns1:Event ;
    skos:definition "A test class" .

ns1:activityTimestamp a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:domain ns1:Event ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the event occurred" .

ns1:advertiserDomain a owl:DatatypeProperty ;
    rdfs:label "advertiser domain"@en ;
    rdfs:domain ns1:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Domain of the advertiser" .

ns1:amount a owl:DatatypeProperty ;
    rdfs:label "amount"@en ;
    rdfs:domain ns1:Transaction ;
    rdfs:range xsd:decimal ;
    skos:definition "Transaction amount" .

ns1:appliesToTenant a owl:DatatypeProperty ;
    rdfs:label "applies to tenant"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:string ;
    skos:definition "Tenant identifier this threshold configuration applies to"@en .

ns1:attributedTo a owl:ObjectProperty ;
    rdfs:label "attributed to"@en ;
    rdfs:domain ns1:AttributedInstall ;
    rdfs:range ns1:Campaign ;
    skos:definition "Relates an install to the campaign that drove it" .

ns1:belongsToPlayer owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#belongsToPlayer> .

ns1:bidPrice a owl:DatatypeProperty ;
    rdfs:label "bid price"@en ;
    rdfs:domain ns1:Bid ;
    rdfs:range xsd:decimal ;
    skos:definition "Price of the bid in the ad auction" .

ns1:businessDefinition a owl:AnnotationProperty ;
    rdfs:label "business definition"@en ;
    rdfs:comment "Plain-language business definition of a concept (multi-language supported via language tags)."@en .

ns1:businessImpact a owl:AnnotationProperty ;
    rdfs:label "business impact"@en ;
    rdfs:comment "Business value/impact statement for the concept (e.g., HIGH/MEDIUM/LOW with rationale)."@en .

ns1:businessRationale a owl:AnnotationProperty ;
    rdfs:label "business rationale"@en ;
    rdfs:comment "Why this concept matters from a business perspective (multi-language supported via language tags)."@en .

ns1:businessTerms a owl:AnnotationProperty ;
    rdfs:label "business terms"@en ;
    rdfs:comment "Business terminology/synonyms used for the concept (multi-language supported via language tags)."@en .

ns1:businessThreshold a owl:AnnotationProperty ;
    rdfs:label "business threshold"@en ;
    rdfs:comment "Business thresholds/criteria used to operationalize the concept (multi-language supported via language tags)."@en .

ns1:businessUseCase a owl:AnnotationProperty ;
    rdfs:label "business use case"@en ;
    rdfs:comment "Real-world business use cases for the concept (multi-language supported via language tags)."@en .

ns1:campaignId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "campaign ID"@en ;
    rdfs:domain ns1:Campaign ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a campaign" .

ns1:containsBid a owl:ObjectProperty ;
    rdfs:label "contains bid"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range ns1:Bid ;
    skos:definition "Relates a session to bids that occurred during it" .

ns1:containsEvent owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#containsEvent> .

ns1:contributedBy a owl:AnnotationProperty ;
    rdfs:label "contributed by"@en ;
    rdfs:comment "Provenance: identifier of the contributor (domain expert, ontology editor, etc.)."@en .

ns1:contributionDate a owl:AnnotationProperty ;
    rdfs:label "contribution date"@en ;
    rdfs:comment "Provenance: date of contribution (xsd:date)."@en .

ns1:country a owl:DatatypeProperty ;
    rdfs:label "country"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:string ;
    skos:definition "Country code of the device" .

ns1:currency a owl:DatatypeProperty ;
    rdfs:label "currency"@en ;
    rdfs:domain ns1:Transaction ;
    rdfs:range xsd:string ;
    skos:definition "Currency code for the transaction" .

ns1:deviceId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "device ID"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:string ;
    owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#deviceId> ;
    skos:definition "Unique identifier for a device" .

ns1:deviceOS a owl:DatatypeProperty ;
    rdfs:label "device OS"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:string ;
    skos:definition "Operating system of the device" .

ns1:deviceType a owl:DatatypeProperty ;
    rdfs:label "device type"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:string ;
    skos:definition "Type of device (phone, tablet, etc.)" .

ns1:dolphinIapThreshold a owl:DatatypeProperty ;
    rdfs:label "dolphin IAP threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Dolphin"@en .

ns1:dolphinMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "dolphin monetary threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Dolphin (future use)"@en .

ns1:eventType a owl:DatatypeProperty ;
    rdfs:label "event type"@en ;
    rdfs:domain ns1:Event ;
    rdfs:range xsd:string ;
    skos:definition "Type of event" .

ns1:hasAdInteraction a owl:ObjectProperty ;
    rdfs:label "has ad interaction"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range ns1:AdInteraction ;
    skos:definition "Relates a session to ad interactions during it" .

ns1:hasAttributedInstall a owl:ObjectProperty ;
    rdfs:label "has attributed install"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:AttributedInstall ;
    skos:definition "Relates a device to its attributed install event" .

ns1:hasEngagementLevel a owl:ObjectProperty ;
    rdfs:label "has engagement level"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:EngagementLevel ;
    owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#hasEngagementLevel> ;
    skos:definition "Relates a device to its inferred engagement classification" .

ns1:hasImpression a owl:ObjectProperty ;
    rdfs:label "has impression"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range ns1:Impression ;
    skos:definition "Relates a session to ad impressions shown during it" .

ns1:hasInAppPurchase a owl:ObjectProperty ;
    rdfs:label "has in-app purchase"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range ns1:InAppPurchase ;
    skos:definition "Relates a session to purchases made during it" .

ns1:hasMonetizationSegment a owl:ObjectProperty ;
    rdfs:label "has monetization segment"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:MonetizationSegment ;
    skos:definition "Relates a device to its inferred monetization classification" .

ns1:hasRetentionStatus a owl:ObjectProperty ;
    rdfs:label "has retention status"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:RetentionStatus ;
    owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#hasRetentionStatus> ;
    skos:definition "Relates a device to its inferred retention status" .

ns1:hasSession a owl:ObjectProperty ;
    rdfs:label "has session"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:Session ;
    owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#hasSession> ;
    owl:inverseOf ns1:belongsToDevice ;
    skos:definition "Relates a device to its sessions" .

ns1:lastReviewed a owl:AnnotationProperty ;
    rdfs:label "last reviewed"@en ;
    rdfs:comment "Provenance: date this business semantics annotation was last reviewed (xsd:date)."@en .

ns1:minnowIapThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow IAP threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Minnow"@en .

ns1:minnowMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "minnow monetary threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Minnow (future use)"@en .

ns1:occursInGame a owl:ObjectProperty ;
    rdfs:label "occurs in game"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range ns1:Game ;
    skos:definition "Relates a session to the game it occurs in" .

ns1:occursInSession owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#occursInSession> .

ns1:partOfCampaign a owl:ObjectProperty ;
    rdfs:label "part of campaign"@en ;
    rdfs:domain ns1:AdEvent ;
    rdfs:range ns1:Campaign ;
    skos:definition "Relates an ad event to its campaign" .

ns1:playsGame a owl:ObjectProperty ;
    rdfs:label "plays game"@en ;
    rdfs:comment "Inferred from hasSession and occursInGame" ;
    rdfs:domain ns1:Device ;
    rdfs:range ns1:Game ;
    skos:definition "Relates a device to games it plays" .

ns1:sessionCount a owl:DatatypeProperty ;
    rdfs:label "session count"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:integer ;
    skos:definition "Total number of sessions for this device" .

ns1:sessionDuration owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#sessionDuration> .

ns1:sessionId a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "session ID"@en ;
    rdfs:domain ns1:Session ;
    rdfs:range xsd:string ;
    owl:equivalentProperty <http://ontology.gaming.network/universal/gaming#sessionId> ;
    skos:definition "Unique identifier for a session" .

ns1:testProperty a owl:DatatypeProperty ;
    rdfs:label "Test Property"@en ;
    rdfs:domain ns1:TestClass ;
    rdfs:range <http://www.w3.org/2001/XMLSchema#xsd:string> ;
    skos:definition "Test property for E2E testing" .

ns1:totalSpent a owl:DatatypeProperty ;
    rdfs:label "total spent"@en ;
    rdfs:domain ns1:Device ;
    rdfs:range xsd:decimal ;
    skos:definition "Total amount spent by this device" .

ns1:whaleIapThreshold a owl:DatatypeProperty ;
    rdfs:label "whale IAP threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:integer ;
    skos:definition "Minimum IAP count to classify as Whale"@en .

ns1:whaleMonetaryThreshold a owl:DatatypeProperty ;
    rdfs:label "whale monetary threshold"@en ;
    rdfs:domain ns1:MonetizationThresholds ;
    rdfs:range xsd:decimal ;
    skos:definition "Minimum monetary value (£) to classify as Whale (future use)"@en .

<http://ontology.gaming.network/universal/bridge> a owl:Ontology ;
    rdfs:comment "Alignment axioms mapping core gaming ontology terms to universal foundations." .

<http://ontology.gaming.network/universal/gaming> a owl:Ontology ;
    dc:creator "Gi Fernando" ;
    dc:date "2025-12-27" ;
    dc:title "Universal Gaming Foundation Ontology v1.0" ;
    rdfs:comment "Comprehensive gaming ontology extending universal human behavior foundation" ;
    owl:imports <http://ontology.gaming.network/universal/human_behavior> ;
    owl:versionInfo "1.0.0" .

<http://ontology.gaming.network/universal/gaming#AdViewed> a owl:Class ;
    rdfs:label "Ad Viewed"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#MonetizationEvent> ;
    skos:definition "Advertisement impression event" .

<http://ontology.gaming.network/universal/gaming#GameEnd> a owl:Class ;
    rdfs:label "Game End"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    skos:definition "Game completion event" .

<http://ontology.gaming.network/universal/gaming#GameStart> a owl:Class ;
    rdfs:label "Game Start"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    skos:definition "Game initialization event" .

<http://ontology.gaming.network/universal/gaming#LevelComplete> a owl:Class ;
    rdfs:label "Level Complete"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    skos:definition "Level completion event" .

<http://ontology.gaming.network/universal/gaming#LevelFail> a owl:Class ;
    rdfs:label "Level Fail"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    skos:definition "Level failure event" .

<http://ontology.gaming.network/universal/gaming#LevelStart> a owl:Class ;
    rdfs:label "Level Start"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    skos:definition "Level initialization event" .

<http://ontology.gaming.network/universal/gaming#ProgressionEvent> a owl:Class ;
    rdfs:label "Progression Event"@en ;
    rdfs:comment "Events related to player progression." ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#GameEvent> ;
    skos:definition "Progression events (LevelUp, SkillUnlock, Milestone)" .

<http://ontology.gaming.network/universal/gaming#Purchase> a owl:Class ;
    rdfs:label "Purchase"@en ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#MonetizationEvent> ;
    skos:definition "Purchase event (in-app purchase, subscription)" .

<http://ontology.gaming.network/universal/gaming#SocialEvent> a owl:Class ;
    rdfs:label "Social Event"@en ;
    rdfs:comment "Events related to social interactions in games." ;
    rdfs:subClassOf <http://ontology.gaming.network/universal/gaming#GameEvent> ;
    skos:definition "Social events (MultiplayerJoin, FriendInvite, ChatMessage)" .

<http://ontology.gaming.network/universal/gaming#amount> a owl:DatatypeProperty ;
    rdfs:label "amount"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#MonetizationEvent> ;
    rdfs:range xsd:decimal ;
    skos:definition "Transaction amount" .

<http://ontology.gaming.network/universal/gaming#currency> a owl:DatatypeProperty ;
    rdfs:label "currency"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#MonetizationEvent> ;
    rdfs:range xsd:string ;
    skos:definition "Currency code" .

<http://ontology.gaming.network/universal/gaming#difficulty> a owl:DatatypeProperty ;
    rdfs:label "difficulty"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    rdfs:range xsd:string ;
    skos:definition "Difficulty level (easy, medium, hard)" .

<http://ontology.gaming.network/universal/gaming#engagementScore> a owl:DatatypeProperty ;
    rdfs:label "engagement score"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range xsd:decimal ;
    skos:definition "Calculated engagement metric" .

<http://ontology.gaming.network/universal/gaming#eventType> a owl:DatatypeProperty ;
    rdfs:label "event type"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#GameEvent> ;
    rdfs:range xsd:string ;
    skos:definition "Type of gaming event" .

<http://ontology.gaming.network/universal/gaming#gameGenre> a owl:DatatypeProperty ;
    rdfs:label "game genre"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Game> ;
    rdfs:range xsd:string ;
    skos:definition "Genre of the game (Action, RPG, Puzzle, etc.)" .

<http://ontology.gaming.network/universal/gaming#gameId> a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "game ID"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Game> ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a game" .

<http://ontology.gaming.network/universal/gaming#gameName> a owl:DatatypeProperty ;
    rdfs:label "game name"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Game> ;
    rdfs:range xsd:string ;
    skos:definition "Name of the game" .

<http://ontology.gaming.network/universal/gaming#hasMonetizationSegment> a owl:ObjectProperty ;
    rdfs:label "has monetization segment"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#MonetizationSegment> ;
    skos:definition "Relates a player to their monetization segment classification" .

<http://ontology.gaming.network/universal/gaming#hasPlayerState> a owl:ObjectProperty ;
    rdfs:label "has player state"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#PlayerState> ;
    skos:definition "Relates a player to their current state" .

<http://ontology.gaming.network/universal/gaming#itemId> a owl:DatatypeProperty ;
    rdfs:label "item ID"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#MonetizationEvent> ;
    rdfs:range xsd:string ;
    skos:definition "Item/product identifier" .

<http://ontology.gaming.network/universal/gaming#levelNumber> a owl:DatatypeProperty ;
    rdfs:label "level number"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#EngagementEvent> ;
    rdfs:range xsd:integer ;
    skos:definition "Level identifier/number" .

<http://ontology.gaming.network/universal/gaming#occursInGame> a owl:ObjectProperty ;
    rdfs:label "occurs in game"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#GameSession> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#Game> ;
    skos:definition "Relates a gaming session to the game it occurs in" .

<http://ontology.gaming.network/universal/gaming#playsGame> a owl:ObjectProperty ;
    rdfs:label "plays game"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#Game> ;
    skos:definition "Relates a player to games they play" .

<http://ontology.gaming.network/universal/gaming#retentionDay> a owl:DatatypeProperty ;
    rdfs:label "retention day"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range xsd:integer ;
    skos:definition "Day X retention status (1, 7, 30)" .

<http://ontology.gaming.network/universal/gaming#sessionCount> a owl:DatatypeProperty ;
    rdfs:label "session count"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range xsd:integer ;
    skos:definition "Number of gaming sessions for this player" .

<http://ontology.gaming.network/universal/gaming#similarBehaviorTo> a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar behavior to"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#Player> ;
    skos:definition "Relates players with similar behavioral patterns (across games)" .

<http://ontology.gaming.network/universal/gaming#similarEventTypeTo> a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar event type to"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#GameEvent> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#GameEvent> ;
    skos:definition "Relates similar event types across games" .

<http://ontology.gaming.network/universal/gaming#similarGenreTo> a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "similar genre to"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Game> ;
    rdfs:range <http://ontology.gaming.network/universal/gaming#Game> ;
    skos:definition "Relates games with similar genres" .

<http://ontology.gaming.network/universal/gaming#totalIaps> a owl:DatatypeProperty ;
    rdfs:label "total IAPs"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/gaming#Player> ;
    rdfs:range xsd:integer ;
    skos:definition "Total in-app purchases for this player" .

<http://ontology.gaming.network/universal/human_behavior#activityDuration> a owl:DatatypeProperty ;
    rdfs:label "activity duration"@en ;
    rdfs:comment "Universal temporal property for activity duration." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#Activity> ;
    rdfs:range xsd:integer ;
    skos:definition "Duration of the activity in seconds" .

<http://ontology.gaming.network/universal/human_behavior#activityEndTimestamp> a owl:DatatypeProperty ;
    rdfs:label "activity end timestamp"@en ;
    rdfs:comment "Universal temporal property for activity end time." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#Activity> ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity ended" .

<http://ontology.gaming.network/universal/human_behavior#activityId> a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "activity ID"@en ;
    rdfs:comment "Universal identifier property for any activity type." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#Activity> ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for an activity" .

<http://ontology.gaming.network/universal/human_behavior#activityStartTimestamp> a owl:DatatypeProperty ;
    rdfs:label "activity start timestamp"@en ;
    rdfs:comment "Universal temporal property for activity start time." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#Activity> ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the activity started" .

<http://ontology.gaming.network/universal/human_behavior#activityTimestamp> a owl:DatatypeProperty ;
    rdfs:label "activity timestamp"@en ;
    rdfs:comment "Universal temporal property for any behavioral event type." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#BehavioralEvent> ;
    rdfs:range xsd:dateTime ;
    skos:definition "Timestamp when the behavioral event occurred" .

<http://ontology.gaming.network/universal/human_behavior#actorId> a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "actor ID"@en ;
    rdfs:comment "Universal identifier property for any human actor type." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#HumanActor> ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a human actor" .

<http://ontology.gaming.network/universal/human_behavior#eventId> a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "event ID"@en ;
    rdfs:comment "Universal identifier property for any behavioral event type." ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#BehavioralEvent> ;
    rdfs:range xsd:string ;
    skos:definition "Unique identifier for a behavioral event" .

<http://ontology.gaming.network/universal/human_behavior#generatesEvent> a owl:ObjectProperty ;
    rdfs:label "generates event"@en ;
    rdfs:domain <http://ontology.gaming.network/universal/human_behavior#Activity> ;
    rdfs:range <http://ontology.gaming.network/universal/human_behavior#BehavioralEvent> ;
