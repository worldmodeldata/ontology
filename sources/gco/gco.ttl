    rdfs:isDefinedBy : ;
    rdfs:range :Character .

:age a owl:DatatypeProperty ;
    rdfs:label "age"@en ;
    rdfs:comment """A character's age value.

Note: This value indicates that the character does not age during a game, and the whole game is set during the year of the character's age. %The age can be calculated if the character's birth date or birth year is known -- see gco:birthDate or gco:birthYear respectively."""@en ;
    rdfs:domain :Character ;
    rdfs:isDefinedBy : ;
    rdfs:range xsd:int .

:ancestorOf a owl:ObjectProperty ;
    rdfs:label "ancestor of"@en ;
    rdfs:comment "Defines that a character is an ancestor of another character."@en ;
    rdfs:domain :Relationship ;
    rdfs:isDefinedBy : ;
    rdfs:range :Character ;
    owl:inverseOf :descendantOf .

:appearsIn a owl:ObjectProperty ;
    rdfs:label "appears in"@en ;
    rdfs:comment "Specifies the game(s) in which the character appears."@en ;
    rdfs:domain :Character ;
    rdfs:isDefinedBy : ;
    rdfs:range cgo:Game .

