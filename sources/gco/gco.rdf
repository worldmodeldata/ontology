        <rdfs:comment xml:lang="en">Defines a boolean whether an armour can be wearable or not.</rdfs:comment>        
    </owl:DatatypeProperty>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#birthDate -->

    <owl:DatatypeProperty rdf:about="http://autosemanticgame.eu/ontologies/gco#birthDate">
        <rdfs:domain rdf:resource="http://autosemanticgame.eu/ontologies/gco#Character"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label xml:lang="en">birth date</rdfs:label>        
        <rdfs:isDefinedBy rdf:resource="http://autosemanticgame.eu/ontologies/gco#"/>
        <rdfs:comment xml:lang="en">Specifies the character&apos;s birth date value as an xsd:date in the format YYYY-MM-DD. This value, together with a game&apos;s date at any point in a game, could be used to calculate the character&apos;s age.</rdfs:comment>   
    </owl:DatatypeProperty>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#birthYear -->

    <owl:DatatypeProperty rdf:about="http://autosemanticgame.eu/ontologies/gco#birthYear">
        <rdfs:domain rdf:resource="http://autosemanticgame.eu/ontologies/gco#Character"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
