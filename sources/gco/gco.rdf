		<dc:description rdf:resource="http://autosemanticgame.eu/ontologies/gco/diagram/gco.svg"/>
        <vann:preferredNamespaceUri>http://autosemanticgame.eu/ontologies/gco#</vann:preferredNamespaceUri>
    </owl:Ontology>
    


