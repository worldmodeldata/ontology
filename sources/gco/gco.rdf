    <!-- http://autosemanticgame.eu/ontologies/gco#Ability -->

    <owl:Class rdf:about="http://autosemanticgame.eu/ontologies/gco#Ability">
        <rdfs:label xml:lang="en">Ability</rdfs:label>
        <rdfs:comment xml:lang="en">Passive actions that are applied automatically and are not activated by a player.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://autosemanticgame.eu/ontologies/gco#"/>        
    </owl:Class>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#Armour -->

    <owl:Class rdf:about="http://autosemanticgame.eu/ontologies/gco#Armour">
        <rdfs:label xml:lang="en">Armour</rdfs:label>
        <rdfs:comment xml:lang="en">Defines a piece of armour which characters can use or wear to deflect any damage (i.e. minimise hit points) inflicted by opponents.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://autosemanticgame.eu/ontologies/gco#"/>
    </owl:Class>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#Character -->

    <owl:Class rdf:about="http://autosemanticgame.eu/ontologies/gco#Character">
        <rdfs:label xml:lang="en">Character</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://autosemanticgame.eu/ontologies/gco#"/>
        <rdfs:comment xml:lang="en">Any character within a video game. The character could be playable or non playable (NPC).</rdfs:comment>
    </owl:Class>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#CharacterClass -->

    <owl:Class rdf:about="http://autosemanticgame.eu/ontologies/gco#CharacterClass">
        <rdfs:label xml:lang="en">Character Class</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://autosemanticgame.eu/ontologies/gco#"/>
        <rdfs:comment xml:lang="en">Defines a more specific classification of characters based on archetypes and/or careers.</rdfs:comment>        
    </owl:Class>
    


    <!-- http://autosemanticgame.eu/ontologies/gco#Female -->

    <owl:Class rdf:about="http://autosemanticgame.eu/ontologies/gco#Female">
