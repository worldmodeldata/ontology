        <rdfs:comment xml:lang="en">Defines a character being a Female.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://autosemanticgame.eu/ontologies/gco#Gender"/>
        <owl:disjointWith rdf:resource="http://autosemanticgame.eu/ontologies/gco#Male"/>
    </owl:Class>



    <!-- http://autosemanticgame.eu/ontologies/gco#Garment -->

