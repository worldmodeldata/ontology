    <!-- http://autosemanticgame.eu/ontologies/gco#age -->

    <owl:DatatypeProperty rdf:about="http://autosemanticgame.eu/ontologies/gco#age">
        <rdfs:domain rdf:resource="http://autosemanticgame.eu/ontologies/gco#Character"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
